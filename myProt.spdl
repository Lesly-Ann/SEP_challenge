hashfunction h;
usertype Msg;
usertype MySecret;

protocol myProtocol(A,B)
{
	role A
	{
		fresh K: MySecret;
		var n: Nonce;

		send_1(A, B, h(A, K), {K}pk(B));
		recv_2(B, A, h(B, K, n), {n}pk(A));
		send_3(A, B, h(n));
	}	
	
	role B
	{
		var K: MySecret;
		fresh n: Nonce;

		recv_1(A, B, h(A, K), {K}pk(B));
		send_2(B, A, h(B, K, n), {n}pk(A));
		recv_3(A, B, h(n));
	}
}