protocol prot(I,R,S)
{
	role I
	{
    fresh K: Nonce;
		send_1(I,S, I, {R,K}k(I,S));
		recv_3(R,I, R, {R}K);
	}	
	
	role R
	{
		var K: Nonce;

		recv_2(S,R, S, {K,I}k(R,S));
		send_3(R,I, R, {R}K);
	}

	role S
	{
		var K: Nonce;
		
		recv_1(I,S, I, {R,K}k(I,S));
		send_2(S,R, S, {K,I}k(R,S));
	}
}

